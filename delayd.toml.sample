data_dir = "/var/lib/delayd"
log_dir = "/var/log/delayd"

[raft]
single_node = true

[amqp]
url = "amqp://localhost"
qos = 10

[amqp.exchange]
name = "delayd"
kind = "topic"
durable = true
auto_delete = false
internal = false
noWait = false

[amqp.queue]
name = "delayd"
bind = [ "delayd" ]
durable = true
auto_ack = false
exclusive = false
no_local = false
no_wait = false
auto_delete = false

[sqs]
region = "ap-northeast-1"
queue = "delayd"
max_number_messages = 10
visibility_timeout = 10
